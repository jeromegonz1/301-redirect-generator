modules = ["python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Streamlit 301 Redirect Generator"

[[workflows.workflow]]
name = "Streamlit 301 Redirect Generator"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run src/main.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "src/main.py"]
